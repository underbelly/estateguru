{
  "name": "Estate Guru",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "sass": "sass scss/style.scss staging/style.css --watch",
    "sass:build": "sass scss/style.scss staging/style.css",
    "dev": "esbuild js/app.js --bundle --outfile=staging/app.bundle.js --format=iife --global-name=AppModule --watch --platform=browser",
    "build": "npm run sass:build && esbuild js/app.js --bundle --outfile=staging/app.bundle.js --format=iife --global-name=AppModule --minify --platform=browser",
    "build-esm": "esbuild js/app.js --bundle --outfile=staging/app.bundle.js --format=esm --minify --platform=browser",
    "dev:all": "concurrently \"npm run sass\" \"npm run dev\"",
    "start": "npm run dev:all",
    "promote": "node scripts/promote.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "esbuild": "^0.19.0",
    "sass": "^1.89.0"
  },
  "dependencies": {
    "gsap": "^3.13.0",
    "lenis": "^1.3.9",
    "smooothy": "^0.0.26"
  }
